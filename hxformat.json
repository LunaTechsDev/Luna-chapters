{
  "wrapping": {
    "arrayMatrixWrap": "matrixWrapWithAlign",
    "arrayWrap": {
      "defaultWrap": "fillLine",
      "defaultLocation": "afterLast"
    },
    "maxLineLength": 80
  },
  "lineEnds": {
    "leftCurly": "after",
    "emptyCurly": "break"
  },
  "indentation": {
    "character": " ",
    "tabWidth": 2
  },
  "whitespace": {
    "typeHintColonPolicy": "after"
  },
  "sameLine": {
    "functionBody": "keep"
  }
}